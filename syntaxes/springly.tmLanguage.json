{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Springly",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#comments"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#identifiers"
		}
	],
	"repository": {
		"comments": {
			"patterns": [
				{
					"name": "comment.line.number-sign",
					"begin": "(\\#)",
					"end": "(\\n$)"
				}
			]
		},
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.springly",
					"match": "\\b(use|test case|open|close|navigate|click|double|right|to|on|in|browser)\\b"
				}
			]
		},
		"identifiers": {
			"name": "string.quoted.double.springly",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.double.springly",
					"match": "\\\\."
				}
			]
		},
		"strings": {
			"name": "string.quoted.single.springly",
			"begin": "'",
			"end": "'",
			"patterns": [
				{
					"name": "constant.character.escape.springly",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.spr"
}